<!doctype html>
<html>
<head>
	<title>VCFL</title>
	<link rel="stylesheet" href="styles.css">
	
</head>
<body>
	<div><img src="banner.png" onclick="window.location='/'" class="bannerImage"></img></div>
	<!-- Logged in as -->
	<div class="loginUser">
		{{#if loginUserName}}
			logged in as:{{loginUserName}}
		{{/if}}
	</div>
	
	<!-- Body -->
	{{{body}}}

	<!-- Error Status -->
	<div>
		{{#if errStatus}}
			<p>{{errStatus}}</p>
		{{/if}}
	</div>
</body>
</html>